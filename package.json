{
  "name": "Rando-Racer",
  "version": "0.0.0",
  "description": "bundle de graficos do sistema de Race para randomizers",
  "homepage": "",
  "author": {
    "name": "Mirage",
    "email": "",
    "url": ""
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "races",
        "title": "Races",
        "width": 3,
        "file": "racesList.html",
        "headerColor": "#0000FF",
        "workspace": "Races"
      },
      {
        "name": "raceSelector",
        "title": "Race Selector",
        "width": 3,
        "file": "raceSelector.html",
        "headerColor": "#0000FF"
      },
      {
        "name": "Telas",
        "title": "Telas",
        "width": 2,
        "file": "telas.html",
        "headerColor": "#0000FF"
      },
      {
        "name": "Videos",
        "title": "Videos",
        "width": 2,
        "file": "videos.html",
        "headerColor": "#0000FF"
      },
      {
        "name": "host",
        "title": "Nova Run",
        "width": 3,
        "file": "newRun.html",
        "headerColor": "#0000FF",
        "workspace": "Races"
      },
      {
        "name": "Tracker",
        "title": "Tracker",
        "width": 5,
        "file": "randoTracker.html",
        "headerColor": "#0000FF",
        "workspace": "Tracker"
      },
      {
        "name": "layout",
        "title": "Stream Layout",
        "width": 5,
        "file": "streamLayout.html",
        "headerColor": "#0000FF",
        "workspace": "Scene Editing"
      },
      {
        "name": "Timer",
        "title": "Timer",
        "width": 3,
        "file": "timer.html",
        "headerColor": "#0000FF"
      },
      {
        "name": "setLayout",
        "title": "Layout",
        "file": "setLayout.html",
        "width": 6,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "save",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ]
      },
      {
        "name": "setCrop",
        "title": "Crop",
        "file": "setCrop.html",
        "width": 6,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "save",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ]
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 963
      },
      {
        "file": "tracker.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "hosts.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "syncvideos.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "sync.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "async.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "crop.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT",
  "dependencies": {
    "bower-npm-resolver": "^0.11.0",
    "fs": "0.0.1-security",
    "livesplit-core": "^0.10.2",
    "nodecg-utility-obs": "github:rodrigo-mirage/nodecg-utility-obs",
    "twitch-m3u8": "^1.1.5"
  }
}
